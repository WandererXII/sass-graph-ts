{
  "name": "sass-graph-ts",
  "version": "5.0.0",
  "description": "Parse sass files and extract a graph of imports",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WandererXII/sass-graph-ts.git"
  },
  "author": "xzyfer, WandererXII (ts rewrite)",
  "type": "module",
  "main": "dist/sass-graph.js",
  "module": "dist/sass-graph.js",
  "types": "dist/sass-graph.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sass-graph.d.ts",
      "import": "./dist/sass-graph.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "test": "mocha",
    "build": "tsc",
    "lint": "oxlint src",
    "format": "prettier --cache --write ."
  },
  "keywords": [
    "sass",
    "scss",
    "graph"
  ],
  "dependencies": {
    "fast-glob": "^3",
    "scss-tokenizer": "^0.4.3",
    "yargs": "^17.7"
  },
  "devDependencies": {
    "assert": "^2.1",
    "chai": "^5",
    "mocha": "^10",
    "@types/node": "^18",
    "typescript": "^5",
    "oxlint": "^0.15",
    "prettier": "^3"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "src",
    "bin",
    "dist"
  ]
}
